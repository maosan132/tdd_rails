<h1>Listing <%= pluralize(@players.length, 'player') %></h1>

<table>
  <thead>
    <tr>
      <td>First name</td>
      <td>Last name</td>
      <td>Actions</td>
    </tr>
  </thead>
  <tbody>
  <% @players.each do |p| %>
    <tr>
      <td><%= p.first_name %></td>
      <td><%= p.last_name %></td>
      <%
=begin%>
 <td><%= link_to  "Show profile", player_path(p.id) %> |
          <%= link_to  "Edit profile", player_edit_path(p.id) %> |
          <%= link_to  "Delete profile", player_path(p.id), data: {method: :delete, confirm: 'r u sure?'} %>
      </td> 
<%
=end%>
    </tr>
    <% end %>
  </tbody>
</table>
<p>Eduardo Baik</p>
<p>Cris Burns</p>
